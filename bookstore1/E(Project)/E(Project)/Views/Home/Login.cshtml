@model Registration
<!doctype html>
<html lang="zxx">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>aranoz</title>

</head>

<body>
    <!--::header part start::-->
    @Html.Partial("Navbar")
    <!-- Header part end-->
    <!-- breadcrumb start-->
    <section class="breadcrumb breadcrumb_bg">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-8">
                    <div class="breadcrumb_iner">
                        <div class="breadcrumb_iner_item">
                            <h2>login</h2>
                            <p>Home <span>-</span> Tracking Order</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- breadcrumb start-->
    <!--================login_part Area =================-->
    <section class="login_part padding_top">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-6 col-md-6">
                    <div class="login_part_text text-center">
                        <div class="login_part_text_iner">
                            <h2>Registration</h2>
                            <p>
                                a few fields where users type in their username, email address, password, and more
                            </p>
                            <a asp-controller="Home" asp-action="Registration" class="btn_3">Registration</a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6">
                    <div class="login_part_form">
                        <div class="login_part_form_iner">
                            <h3>
                                Welcome Back ! <br>
                                Please login
                            </h3>

                            @if (TempData.ContainsKey("success"))
                            {
                                <p class="text-success">
                                    @TempData["success"]
                                </p>
                            }

                            <form class="row contact_form" action="#" method="post" novalidate="novalidate">

                                <div class="col-md-12 form-group p_star">
                                    <input type="text" class="form-control" asp-for="Email" value=""
                                           placeholder="Email">
                                    <span class="text-danger" asp-validation-for="Email"></span>
                                </div>
                                <div class="col-md-12 form-group p_star">
                                    <input type="password" class="form-control" asp-for="Password" value=""
                                           placeholder="Password">
                                    <span class="text-danger" asp-validation-for="Password"></span>
                                </div>
                                <div class="col-md-12 form-group">
                                    <div class="creat_account d-flex align-items-center">
                                        <input type="checkbox" name="selector">
                                        <label for="f-option">Remember me</label>
                                    </div>
                                    <button type="submit" value="submit" class="btn_3">
                                        log in
                                    </button>
                                    <a class="lost_pass" href="#">forget password?</a>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--================login_part end =================-->
    <!--::footer_part start::-->
    <footer class="footer_part" style="background-color:#229799">
        <div class="container">
            <div class="row justify-content-around">
                <div class="col-sm-6 col-lg-2">
                    <div class="single_footer_part">
                        <h3 style="color:burlywood;">Contents</h3>
                        <ul class="list-unstyled" style="color:azure;">
                            <li><a asp-action="Index" asp-controller="Home">Home</a></li>
                            <li><a asp-action="About" asp-controller="Home">About</a></li>
                            <li><a asp-action="Contact" asp-controller="Home">Contact</a></li>

                        </ul>
                    </div>
                </div>
                <div class="col-sm-6 col-lg-2">
                    <div class="single_footer_part">
                        <h3 style="color:burlywood;">Categories</h3>
                        <ul class="list-unstyled" style="color:azure;">
                            <li><a asp-action="Novel" asp-controller="Home">Novels</a></li>
                            <li><a asp-action="Books" asp-controller="Home">Books</a></li>
                            <li><a asp-action="Stationary" asp-controller="Home">Stationary</a></li>
                            <li><a asp-action="Magzines" asp-controller="Home">Magzines</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-sm-6 col-lg-2">
                    <div class="single_footer_part">
                        <h3 style="color:burlywood;">Support</h3>
                        <ul class="list-unstyled">
                            <li><a asp-action="Index" asp-controller="Home">FAQ's</a></li>
                            <li><a asp-action="Index" asp-controller="Home">Track Order</a></li>

                        </ul>
                    </div>
                </div>
                <div class="col-sm-6 col-lg-2">
                    <div class="single_footer_part">
                        <h3 style="color:burlywood;">Sort By</h3>
                        <ul class="list-unstyled">
                            <li><a asp-action="Index" asp-controller="Home">New Arrivals</a></li>
                            <li><a asp-action="Index" asp-controller="Home">Best Sellers</a></li>
                            <li><a asp-action="Index" asp-controller="Home">sale</a></li>

                        </ul>
                    </div>
                </div>
                <div class="col-sm-6 col-lg-4">
                    <div class="single_footer_part">
                        <h3 style="color:burlywood;">Newsletter</h3>
                        <p style="color:azure;">
                            Heaven fruitful doesn't over lesser in days. Appear creeping
                        </p>
                        <div id="mc_embed_signup">
                            <form target="_blank"
                                  action="https://spondonit.us12.list-manage.com/subscribe/post?u=1462626880ade1ac87bd9c93a&amp;id=92a4423d01"
                                  method="get" class="subscribe_form relative mail_part">
                                <input type="email" name="email" id="newsletter-form-email" placeholder="Email Address"
                                       class="placeholder hide-on-focus" onfocus="this.placeholder = ''"
                                       onblur="this.placeholder = ' Email Address '">
                                <button type="submit" name="submit" id="newsletter-submit"
                                        class="email_icon newsletter-submit button-contactForm">
                                    subscribe
                                </button>
                                <div class="mt-10 info"></div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>

        </div>
        <div class="copyright_part">
            <div class="container">
                <div class="row">
                    <div class="col-lg-8">
                        <div class="copyright_text">
                        </div>
                    </div>
                    <div class="col-lg-4">
                        <div class="footer_icon social_icon">
                            <ul class="list-unstyled">
                                <li><a href="https://www.facebook.com/login/" class="single_social_icon"><i class="bi bi-facebook"></i></a></li>
                                <li><a href="https://www.instagram.com/" class="single_social_icon"><i class="bi bi-instagraqm"></i></a></li>
                                <li><a href="https://www.youtube.com/" class="single_social_icon"><i class="bi bi-youtube"></i></a></li>
                                <li><a href="https://www.whatsapp.com/" class="single_social_icon"><i class="bi bi-whatsapp"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!--::footer_part end::-->
    <!-- jquery plugins here-->
    <!-- jquery -->

</body>

</html>