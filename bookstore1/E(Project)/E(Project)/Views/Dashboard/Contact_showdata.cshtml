@model IEnumerable<E_Project_.Models.Contact>

@{
    Layout = null;
    Layout = "~/Views/Shared/Dashboardlink.cshtml";
}
@Html.Partial("Dashboardnavbar")
<table class="table table-striped table-hover shadow-lg p-3 mb-5 ">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Message </th>
            <th scope="col">Name</th>
            <th scope="col"> Email</th>
            <th scope="col">Subject</th>
            <th scope="col">Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Message </td>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Subject</td>
                <td><a type="button" class="btn btn-danger" asp-action="Contact_delete" asp-route-id="@item.Id">Delete</a></td>
            </tr>
        }

    </tbody>
</table>
