@model IEnumerable<E_Project_.Models.Registration>

@{
    Layout = null;
    Layout = "~/Views/Shared/Dashboardlink.cshtml";
}
@Html.Partial("Dashboardnavbar")
<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name </th>
            <th scope="col"> Email</th>
            <th scope="col">Password</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Password</td>
                <td><a type="button" class="btn btn-danger" asp-action="Registration_delete" asp-route-id="@item.Id">Delete</a></td>
            </tr>
        }

    </tbody>
</table>